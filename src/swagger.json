  "paths": {
    "/users": {
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Register a user",
        "description": "Create a new user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              },
              "example": {
                "name": "Antonio",
                "email": "arfs.antonio@gmail.com",
                "password": "123456"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Error when create new User on API"
          }
        }
      }
    }
  }
}